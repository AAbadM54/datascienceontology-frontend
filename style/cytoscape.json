[
  {
    "selector": "node.graphviz",
    "style": {
      "label": "data(label)",
      "width": "data(width)",
      "height": "data(height)",
      "shape": "roundrectangle",
      "font-size": 12,
      "text-halign": "center",
      "text-valign": "center"
    }
  },
  {
    "selector": "edge.graphviz",
    "style": {
      "label": "",
      "curve-style": "unbundled-bezier",
      "source-endpoint": "data(sourceEndpoint)",
      "target-endpoint": "data(targetEndpoint)",
      "control-point-distances": "data(controlPointDistances)",
      "control-point-weights": "data(controlPointWeights)",
      "width": "1px",
      "font-size": 8
    }
  },
  {
    "selector": "edge.graphviz:selected",
    "style": {
      "label": "data(label)",
      "text-background-color": "white",
      "text-background-opacity": 1,
      "text-background-shape": "roundrectangle",
      "z-index": 5
    }
  },
  {
    "selector": ".graphviz-invis",
    "style": {
      "visibility": "hidden"
    }
  },
  {
    "selector": "core",
    "style": {
      "active-bg-size": 0
    }
  },
  {
    "selector": "*:active",
    "style": {
      "overlay-padding": 0
    }
  }
]
